import java.util.*;
class pattern
{
public static void main(String args[])
{
Scanner in=new Scanner(System.in);
int n;
System.out.println("N=");
n=in.nextInt(); 
for(int i=1;i<=5;i++)
{
for(int j=i;j<=5;j++)
{
System.out.print("*");
}
System.out.println();
}
}
}

OUTPUT:
D:\jyothi>javac pattern.java

D:\jyothi>java pattern
N=
5
*****
****
***
**
*
