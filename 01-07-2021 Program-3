import java.util.*;
class alphabet
{
public static void main(String args[])
{
Scanner in=new Scanner(System.in);
int r,i,j;
System.out.println("N=");
r=in.nextInt();
int alphabet=65;
for(i=0;i<=r-1;i++)
{
for(j=0;j<=i;j++)
{
System.out.print((char)(alphabet++)+" ");
}
System.out.println();
}
in.close();
}
}


OUTPUT:

D:\jyothi>javac alphabet.java

D:\jyothi>java alphabet
N=
5
A
B C
D E F
G H I J
K L M N O
