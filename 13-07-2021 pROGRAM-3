import java.util.*;
 class Recursion 
{
  public static void main(String[] arg) 
     {
      Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       long fact = multiplyNumbers(n);
        System.out.println("Fact of " + n+ " = " + fact);
    }
    public static long multiplyNumbers(int n)
    {
        if (n >= 1)
            return n * multiplyNumbers(n - 1);
        else
            return 1;
    }
}
